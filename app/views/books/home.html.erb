<h3>Books</h3>



<table class= "table table-hover">
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Author </th>
    <th>Price</th>
    <th>Book Picture</th>
  </tr>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.id %></td>
        <td><%= book.title %></td>
        <td><%= book.author_id %></td>
        <td><%= number_to_currency book.price %></td>
        <td><%= link_to "see picture", book.photo_url %></td>
      </tr>
    <% end %>

</table>
